<% provide(:title,t('person.add')) %>
<% provide(:button_see_text,t('person.add_and_see_button')) %>
<% provide(:button_text,t('add_button')) %>
<% if policy(@person).create? %>
  <h1 class="center">
    <%=t('person.add') %>
  </h1>
  <div class='row'>
    <div class="col-lg-3 col-md-3 col-sm-1 hidden-xs"></div>
    <div class="col-lg-6 col-md-6 col-sm-10 col-xs-12">
      <%= render 'form' %>
    </div>
  </div>
<% end %>
